	year = {2021},
	pages = {298--311},
	file = {Yang et al. - 2021 - Truncating bar graphs persistently misleads viewer.pdf:/Users/db/Zotero/storage/HUTAR3FQ/Yang et al. - 2021 - Truncating bar graphs persistently misleads viewer.pdf:application/pdf},
